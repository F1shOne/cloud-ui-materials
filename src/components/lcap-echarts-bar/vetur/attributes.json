{
  "lcap-echarts-bar/data-source": {
    "type": "Array<Item> | Function | object | DataSource",
    "description": "表格的数据源，数据集对象或者返回数据集的逻辑"
  },
  "lcap-echarts-bar/xAxis": {
    "type": "string",
    "description": "设置维度(x轴)"
  },
  "lcap-echarts-bar/xAxisTitle": {
    "type": "string",
    "description": "设置维度(x轴)标题"
  },
  "lcap-echarts-bar/yAxis": {
    "type": "string",
    "description": "设置指标（y轴），多个指标以逗号分隔"
  },
  "lcap-echarts-bar/yAxisTitle": {
    "type": "string",
    "description": "设置指标(y轴)标题"
  },
  "lcap-echarts-bar/width": {
    "type": "string",
    "description": "设置图宽度"
  },
  "lcap-echarts-bar/height": {
    "type": "string",
    "description": "设置图高度"
  },
  "lcap-echarts-bar/title": {
    "type": "string",
    "description": "设置主标题"
  },
  "lcap-echarts-bar/theme": {
    "type": "string",
    "options": [
      {
        "value": "theme1",
        "title": "科技"
      },
      {
        "value": "theme2",
        "title": "清新"
      },
      {
        "value": "theme3",
        "title": "淡雅"
      },
      {
        "value": "theme4",
        "title": "经典"
      }
    ],
    "description": "设置图表配色方案"
  },
  "lcap-echarts-bar/allowShowLabel": {
    "type": "boolean",
    "description": "设置是否显示标签"
  },
  "lcap-echarts-bar/allowShowHint": {
    "type": "boolean",
    "description": "设置是否显示提示"
  },
  "lcap-echarts-bar/allowShowLegend": {
    "type": "boolean",
    "description": "设置是否显示图例"
  },
  "lcap-echarts-bar/showXAxisLine": {
    "type": "boolean",
    "description": "设置是否显示X轴轴线"
  },
  "lcap-echarts-bar/showXAxisLabel": {
    "type": "boolean",
    "description": "设置是否显示X轴文字标签"
  },
  "lcap-echarts-bar/xAxisLabelRotate": {
    "type": "string",
    "options": [
      {
        "value": "0",
        "title": "横向"
      },
      {
        "value": "90",
        "title": "纵向"
      },
      {
        "value": "45",
        "title": "左倾斜"
      },
      {
        "value": "-45",
        "title": "右倾斜"
      }
    ],
    "description": "设置显示X轴文字标签的方向"
  },
  "lcap-echarts-bar/showYAxisLine": {
    "type": "boolean",
    "description": "设置是否显示Y轴轴线"
  },
  "lcap-echarts-bar/showYAxisLabel": {
    "type": "boolean",
    "description": "设置是否显示Y轴数值标签"
  },
  "lcap-echarts-bar/titleFontSize": {
    "type": "number",
    "description": "设置标题文字字号"
  },
  "lcap-echarts-bar/titleFontStyle": {
    "type": "string",
    "options": [
      {
        "value": "normal",
        "title": "正常"
      },
      {
        "value": "italic",
        "title": "斜体"
      }
    ],
    "description": "设置普通文字字号"
  },
  "lcap-echarts-bar/allowDownload": {
    "type": "boolean",
    "description": "允许用户设置保存图片"
  }
}