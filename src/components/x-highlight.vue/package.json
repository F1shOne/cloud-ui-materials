    "base": "vue",
    "ui": "cloud-ui.vusion",
    "screenshots": [
      "https://unpkg.com/@cloud-ui/x-ace-editor.vue@0.6.3/screenshots/0.png",
      "https://unpkg.com/@cloud-ui/x-ace-editor.vue@0.6.3/screenshots/1.png",
      "https://unpkg.com/@cloud-ui/x-ace-editor.vue@0.6.3/screenshots/2.png",
      "https://unpkg.com/@cloud-ui/x-ace-editor.vue@0.6.3/screenshots/3.png",
      "https://unpkg.com/@cloud-ui/x-ace-editor.vue@0.6.3/screenshots/4.png"
