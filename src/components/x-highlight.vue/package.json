    "prepublishOnly": "npm run build:doc && npm run screenshot && vusion readme && vusion vetur && git add ."
  },
  "vuePlugins": {
    "resolveFrom": "../../../"
  },
  "dependencies": {
    "highlight.js": "^9.15.10"
  },
